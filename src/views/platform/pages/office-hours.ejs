    <script>
      // Function to update the Google Calendar theme
function updateCalendarTheme(theme) {
    const calendarIframe = document.getElementById('appointment-scheduler');
    
    if (!calendarIframe) return; // If calendar iframe doesn't exist
    
    // Get the current src
    let currentSrc = calendarIframe.src;
    
    // Remove any existing color parameters
    currentSrc = currentSrc.replace(/[&?]bgcolor=[^&]+/, '')
                         .replace(/[&?]color=[^&]+/, '')
                         .replace(/[&?]showTitle=[^&]+/, '')
                         .replace(/[&?]showNav=[^&]+/, '')
                         .replace(/[&?]showPrint=[^&]+/, '');
    
    // Add the appropriate color parameters based on theme
    if (theme === 'dark') {
        // Dark theme colors - adjust these hex values to match your dark theme
        const darkParams = 'bgcolor=%23121212&color=%23ffffff&showNav=1&showTitle=0&showPrint=0';
        
        // Add parameters to URL
        if (currentSrc.includes('?')) {
            calendarIframe.src = `${currentSrc}&${darkParams}`;
        } else {
            calendarIframe.src = `${currentSrc}?${darkParams}`;
        }
    } else {
        // Light theme colors - adjust these hex values to match your light theme
        const lightParams = 'bgcolor=%23ffffff&color=%234285F4&showNav=1&showTitle=0&showPrint=0';
        
        // Add parameters to URL
        if (currentSrc.includes('?')) {
            calendarIframe.src = `${currentSrc}&${lightParams}`;
        } else {
            calendarIframe.src = `${currentSrc}?${lightParams}`;
        }
    }
}
    </script>
